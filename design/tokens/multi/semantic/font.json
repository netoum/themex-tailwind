{
  "font": {
    "family": {
      "code": {
        "type": "fontFamily",
        "value": "{theme.font.family.code}"
      },
      "default": {
        "type": "fontFamily",
        "value": "{theme.font.family.default}"
      }
    },
    "line-height": {
      "default": {
        "type": "lineHeight",
        "value": "{theme.font.line-height.default}"
      }
    },
    "size": {
      "default": {
        "type": "fontSize",
        "value": "{theme.font.size.default}"
      },
      "lg": {
        "type": "fontSize",
        "value": "{theme.font.size.lg}"
      },
      "sm": {
        "type": "fontSize",
        "value": "{theme.font.size.sm}"
      },
      "xl": {
        "type": "fontSize",
        "value": "{theme.font.size.xl}"
      }
    }
  }
}